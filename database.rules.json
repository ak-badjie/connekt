{
    "rules": {
        "notifications": {
            "$uid": {
                ".indexOn": [
                    "createdAt",
                    "read"
                ],
                ".read": "auth != null && auth.uid === $uid",
                ".write": "auth != null && auth.uid === $uid"
            }
        },
        ".read": false,
        ".write": false
    }
}